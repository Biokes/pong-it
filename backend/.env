# Backend service environment variables
# Port the backend HTTP + Socket.IO server will listen on
PORT=8080

# Where the frontend is served during local development (used for CORS)
FRONTEND_URL=http://localhost:3000

# URL for the player ranking service used by the backend to proxy ranking requests
# When running the player service locally use: http://localhost:5001
PLAYER_SERVICE_URL=http://localhost:5001

# Node environment
NODE_ENV=development
