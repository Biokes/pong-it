# Frontend environment variables (Create React App requires REACT_APP_ prefix)
# Where to reach the backend server (HTTP and Socket.IO)
REACT_APP_BACKEND_URL=http://localhost:8080

# Player service URL for rankings and game records
REACT_APP_PLAYER_SERVICE_URL=http://localhost:5001

# WalletConnect Project ID (Required for RainbowKit)
# Get yours at: https://cloud.walletconnect.com/
REACT_APP_WALLETCONNECT_PROJECT_ID=a7a5fb8558c4b1ead1760ba15fe79ba8

# Optional: change port used by CRA dev server
PORT=3000

# Use polling instead of file watchers (slower but works with file watcher limits)
CHOKIDAR_USEPOLLING=true

NODE_ENV=development
